{
  "name": "coviride",
  "version": "1.2.0",
  "description": "CoviRide =======",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "php -S 127.0.0.1:8000 -t public",
    "test": "./vendor/bin/kahlan --cc=true --reporter=verbose",
    "testFixAndCoverage": "php vendor/bin/php-cs-fixer.phar fix --using-cache=false src && ./vendor/bin/kahlan --cc=true --coverage=4 --no-colors=true --reporter=verbose",
    "compile": "encore dev",
    "compileAndWatch": "encore dev-server",
    "compileAndWatchVue": "encore dev-server --hot",
    "generateDoc": "php vendor/bin/phpDocumentor.phar -d src -t public/build/doc --title Coviride",
    "postinstall": "wget http://www.phpdoc.org/phpDocumentor.phar -P vendor/bin && wget https://cs.sensiolabs.org/download/php-cs-fixer-v2.phar -O vendor/bin/php-cs-fixer.phar && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/covivo/CoviRide.git"
  },
  "author": "MatthD",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://gitlab.com/covivo/CoviRide/issues"
  },
  "browserslist": [
    "last 2 versions",
    "ios >= 8"
  ],
  "homepage": "https://gitlab.com/covivo/CoviRide#README",
  "devDependencies": {
    "@symfony/webpack-encore": "^0.20.1",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "sass-lint": "^1.12.1",
    "sass-lint-webpack": "^1.0.0",
    "sass-loader": "^7.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "vue-eslint-parser": "^3.2.2",
    "webpack-notifier": "^1.6.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "buefy": "^0.6.7",
    "casual": "^1.5.19",
    "eslint-config-prettier": "^3.0.1",
    "mapbox-gl": "^0.49.0-beta.1",
    "mapbox-gl-vue": "^1.9.0",
    "remove": "^0.1.5",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "vue": "^2.5.17",
    "vue-loader": "^14.2.3",
    "vue-mapbox": "0.0.34",
    "vue-template-compiler": "^2.5.17"
  }
}
