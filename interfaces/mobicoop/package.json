{
  "name": "mobicoop",
  "version": "1.3.0",
  "description": "Mobicoop front-end app",
  "main": "public/index.php",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node runstart.js $1",
    "test": "node src/MobicoopBundle/spec/runtests.js",
    "testAndCoverage": "node src/MobicoopBundle/spec/runtests.js -c",
    "testFixAndCoverage": "php vendor/bin/php-cs-fixer.phar fix --using-cache=false src &&  php vendor/bin/php-cs-fixer.phar fix --using-cache=false src/MobicoopBundle/spec && node src/MobicoopBundle/spec/runtests.js -c",
    "compile": "encore dev",
    "compileAndWatch": "encore dev-server --port 8079",
    "compileAndWatchVue": "encore dev-server --hot --port 8079",
    "postinstall": "composer install && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/mobicoop/mobicoop.git"
  },
  "author": "MatthD",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://gitlab.com/mobicoop/mobicoop/issues"
  },
  "browserslist": [
    "last 2 versions",
    "ios >= 8"
  ],
  "homepage": "https://gitlab.com/mobicoop/mobicoop#README",
  "devDependencies": {
    "@symfony/webpack-encore": "^0.20.1",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "sass-lint": "^1.12.1",
    "sass-lint-webpack": "^1.0.0",
    "sass-loader": "^7.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "vue-eslint-parser": "^3.2.2",
    "webpack-notifier": "^1.6.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "buefy": "^0.6.7",
    "casual": "^1.5.19",
    "commander": "^2.18.0",
    "eslint-config-prettier": "^3.0.1",
    "kuler": "^1.0.0",
    "mapbox-gl": "^0.49.0-beta.1",
    "mapbox-gl-vue": "^1.9.0",
    "remove": "^0.1.5",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "vue": "^2.5.17",
    "vue-loader": "^14.2.3",
    "vue-mapbox": "0.0.34",
    "vue-template-compiler": "^2.5.17"
  }
}
