{% extends '@Mobicoop/base.html.twig' %}

{% block title %}
    {% trans from 'ui' %}assistiveDevices.title{% endtrans %} - {{ app_name }}
{% endblock %}


{% block main %}
    <assistive
        activation-uri="{{ activationUri is defined ? activationUri : null }}"
        assistive-sso-provider="{{ assistiveSsoProvider }}"
        :user="{{app.user|json_encode}}"
    ></assistive>
{% endblock %}
