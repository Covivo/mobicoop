{% extends '@Mobicoop/base.html.twig' %}

{% block title %}
{% if article.id == 10 %}
  {% trans from 'meta' %}meta.faq.title {% endtrans %} | {{ app_name }}
{% elseif article %}
  {{ article.title }}
{% else %}
  Article
{% endif %}  
{% endblock %}


{% block desc %}
{% if article.id == 10 %}
{% trans from 'meta' %}meta.faq.description{% endtrans %}
{% endif %}  
{% endblock %}

{% block stylesheets %}
  {# {{ encore_entry_link_tags("#{assets_prefix ?? ''}article/article") }} #}
{% endblock %}

{% block main %}
<M-article :article-id = "{{ article.id|json_encode }}"/>  
{% endblock %}

