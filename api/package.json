{
  "name": "coviride-api",
  "version": "0.0.0",
  "description": "Mobicoop API ",
  "main": "public/index.php",
  "scripts": {
    "test": "echo \" no test specified\" && exit 0",
    "start": "php bin/console server:run 127.0.0.1:8080",
    "update-db": "php bin/console doctrine:migrations:migrate -n",
