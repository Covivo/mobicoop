{
  "name": "coviride-api",
  "version": "1.0.0",
  "description": "Mobicoop API ",
  "main": "public/index.php",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start" : "bin/console server:run 127.0.0.1:8080",
    "updateDb" : "bin/console doctrine:migrations:migrate -n",
    "testFixAndCoverage": "php vendor/bin/php-cs-fixer.phar fix --using-cache=false src && ./vendor/bin/kahlan --cc=true --coverage=4 --no-colors=true --reporter=verbose",
    "postinstall": "wget http://www.phpdoc.org/phpDocumentor.phar -P vendor/bin && wget https://cs.sensiolabs.org/download/php-cs-fixer-v2.phar -O vendor/bin/php-cs-fixer.phar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/mobicoop/mobicoop-api.git"
  },
  "keywords": [
    "COVIRIDE",
    "MOBICOOP",
    "ROUTE"
  ],
